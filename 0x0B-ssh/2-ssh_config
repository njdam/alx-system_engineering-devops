# Host/Server Ip Address:54.146.66.192 or * for all ip will be applied to it
Host *
  # File location containing Private Key For A Host or Server
  IdentityFile ~/.ssh/school
  # Password Authentication Is Disabled to Enable Change it to Yes
  PasswordAuthentication no
  # Synchronizing local settings between the client and server
  SendEnv LANG LC_*
  # Host/Server ip stored in this file should be hashed for added security
  HashKnownHosts yes
